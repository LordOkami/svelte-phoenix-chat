{
  "name": "assets",
  "version": "1.0.0",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "rollup -c",
    "deploy": "rollup --config",
    "dev": "rollup -c -w",
    "start": "yarn dev",
    "watch": "rollup --config --watch",
    "lint": "eslint --ext js,svelte src",
    "lint:fix": "eslint --fix --ext js,svelte src",
    "prettier": "prettier \"{,!(node_modules)/**/}*.js\"",
    "prettier:fix": "prettier --write \"{,!(node_modules)/**/}*.js\""
  },
  "dependencies": {
    "phoenix": "file:../deps/phoenix",
    "phoenix_html": "file:../deps/phoenix_html",
    "three": "^0.115.0",
    "vanta": "^0.5.14"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-image": "^2.0.4",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-svelte3": "^2.7.3",
    "husky": "^4.2.5",
    "prettier": "^2.0.4",
    "prettier-plugin-svelte": "^0.7.0",
    "pretty-quick": "^2.0.1",
    "rollup": "^1.20.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-livereload": "^1.1.0",
    "rollup-plugin-postcss": "^2.0.5",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "sirv-cli": "^0.4.4",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^3.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
